<div align="center">
  <ol class="breadcrumb">
    <li class="breadcrumb-item"><a href="/teams">Equipes</a></li>
    <li class="breadcrumb-item active"><strong>Equipe: <em><%=@team.title%></em></strong></li>
  </ol>
</div>

<div class="container-fluid">
	<div class="row">
		<div class="col-md-12">
			<div class="col-md-2">			
			</div>
				<div class="col-md-8">
					<div class="jumbotron well">
						<h1><%= @team.title %></h1>
						<p><strong>Descrição:</strong><em> <%=simple_format(@team.description) %></em></p>						
						<hr>
						<div align="center">
						<%= link_to 'OKR', team_okrs_path(@team), class: "btn btn-primary" %>
						<%if current_user.editor %>
						<%= link_to 'Editar', edit_team_path(@team), class: "btn btn-primary" %> 
						<%end%>
						<%= link_to 'Voltar', teams_path, class: "btn btn-primary" %>
						</div>						
					</div>
				</div>
			<div class="col-md-2">			
			</div>
		</div>
	</div>
</div>

  
  



  

